@model IEnumerable<cReg_WebApp.Models.entities.Course>

@{
    ViewData["Title"] = "CourseInfo";
}
<!DOCTYPE html>

<h1>CourseInfo</h1>

<p>Course's Description: blah blah</p>

<table class="table">
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.courseId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.courseName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.courseDescription)
                </td>
                <td>
                    <button onclick="testFunc()">Register</button>
                </td>
            </tr>
        }
    </tbody>
</table>

<div>
    <a asp-action="RegisterPage">Back to Register</a>
</div>